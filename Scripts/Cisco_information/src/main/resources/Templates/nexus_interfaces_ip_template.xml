<config>
    <delim><![CDATA[===]]></delim>

    <values>
        <!-- here we reuse the line pattern, only we pull out different group values -->
        <value id="interface"><![CDATA[[\n\r](.+?), Interface status:]]></value>
        <value id="ip" group="1"><![CDATA[\s+IP\s+address:\s*(.+?), IP subnet: .+/(\d+?) route-preference:]]></value>
        <value id="subnet" group="2"><![CDATA[\s+IP\s+address:\s*(.+?), IP subnet: .+/(\d+?) route-preference:]]></value>
        <value id="acl-in"><![CDATA[Inbound\s+access\s+list\s+is\s+(.+?)[\n\r]]]></value>
        <value id="acl-out"><![CDATA[Outgoing\s+access\s+list\s+is\s+(.+?)[\n\r]]]></value>
    </values>
</config>